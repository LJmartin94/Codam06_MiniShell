VALID:

\e\c\h\o \-\n \t\e\s\t

"e""c""h""o" "-""n" "t""e""s""t"

'e''c''h''o' '-''n' 't''e''s''t'

"e"'c'"h"'o' '-'"n" te's't

"e"'c'"h"'o' "-"'n' '-'"n" "-"'n' te's't

"e"'c'"h"'o' \-"n" te's't

echo -n -nnnnnnnnnnnn -n -n '-n' "-n" test

"e"\c"h"'o' \-"n" te's't

echo "this is a dq: \""x //WRONG

echo 'this is empty: '''x //WRONG

echo 'this is a backslash: \'''x //WRONG

echo 'this is dq-sq: "'"'"x

echo "-n" test

echo "-"n test

echo '-'n test

echo \-n test

echo -"n" test

echo -'n' test

echo -\n test

echo "-n" "-n" test

echo       -n               test

                  echo -n test

echo three " " spaces //WRONG

echo one" "space

echo five      " "    " "        spaces

pw\d;echo -n test

INVALID:

\e\c\h\o\ \-\n \t\e\s\t

\e\c\h\o \-\n\ \t\e\s\t

"e""c"'h''o'' '-n test

"e"'c'"h"'o' '-' "n" te's't //WRONG

echo -n -nnnnnnnnnnnn -n -n '-n' "-n" -nnnnnnnnnd test

echo -n-n-n-n-n-n test

echo --n test

echo "-n -n" test

echo "-n""-n" test

echo" "-n test

echo -n" "test

"echo " -n test

echo "-n " test

"echo "-n test

echo "-n "test

ech o -n test

ech -n test

echoo -n test

ech\o-n test